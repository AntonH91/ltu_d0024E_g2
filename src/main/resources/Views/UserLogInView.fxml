<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="185.0" prefWidth="208.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal"
            fx:controller="dbbg2.view.controllers.user.UserLoginController">
   <TextField fx:id="txtUserName" layoutX="30.0" layoutY="40.0" promptText="Username"/>
   <PasswordField fx:id="pwdPassword" layoutX="30.0" layoutY="75.0" promptText="Password"/>
   <Button fx:id="btnLogin" defaultButton="true" layoutX="125.0" layoutY="120.0" mnemonicParsing="false"
           onAction="#handleLoginClick" text="Login"/>
   <Button fx:id="btnCancel" cancelButton="true" layoutX="63.0" layoutY="120.0" mnemonicParsing="false"
           onAction="#handleCancelClick" text="Cancel"/>
</AnchorPane>
