<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="185.0" prefWidth="208.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbbg2.controllers.user.UserLoginController">
   <children>
      <TextField fx:id="txtUserName" layoutX="30.0" layoutY="40.0" promptText="Username" />
      <PasswordField fx:id="pwdPassword" layoutX="30.0" layoutY="75.0" promptText="Password" />
      <Button fx:id="btnLogin" defaultButton="true" layoutX="125.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleLoginClick" text="Login" />
      <Button fx:id="btnCancel" cancelButton="true" layoutX="63.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleCancelClick" text="Cancel" />
   </children>
</AnchorPane>
